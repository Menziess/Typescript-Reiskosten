{
  "name": "typescript-reiskosten",
  "version": "1.0.0",
  "description": "Reiskosten calculator.",
  "main": "dist/index.js",
  "scripts": {
    "test": "./node_modules/tslint/bin/tslint src/**/*.ts{,x}",
    "start": "nodemon --watch dist dist/app-server.js",
    "electron": "electron dist/app-client.js",
    "watchify": "mkdir -p public/js && watchify dist/index.js -o public/js/bundle.js",
    "browserify": "mkdir -p public/js && browserify dist/index.js -o public/js/bundle.js",
    "compile": "tsc",
    "dev": "concurrently --kill-others \"tsc --watch\" \"npm run watchify\" \"npm run start\"",
    "watch": "concurrently --kill-others \"tsc --watch\" \"npm run watchify\"",
    "postinstall": "./node_modules/.bin/tsc && npm run browserify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Menziess/Typescript-Reiskosten.git"
  },
  "author": "Stefan Schenk",
  "private": true,
  "dependencies": {
    "browserify": "^15.0.0",
    "express": "^4.0.35",
    "express-rewrite": "^0.0.4",
    "typescript": "^2.1.5"
  },
  "devDependencies": {
    "@types/express": "*",
    "@types/lodash": "*",
    "@types/node": "^8.5.7",
    "concurrently": "^3.1.0",
    "nodemon": "^1.2.1",
    "tslint": "^5.8.0",
    "watchify": "^3.9.0"
  },
  "bugs": {
    "url": "https://github.com/Menziess/Typescript-Reiskosten/issues"
  },
  "homepage": "https://github.com/Menziess/Typescript-Reiskosten#readme"
}
