{
  "name": "typescript-reiskosten",
  "version": "1.0.0",
  "description": "Taxiride price calculator",
  "main": "dist/index.js",
  "scripts": {
    "pretest": "./node_modules/.bin/tslint src/**/*.ts{,x} --config tslint.json --project tsconfig.json",
    "test": "./node_modules/.bin/mocha -r ts-node/register test/**/*.spec.ts --exit",
    "start": "node dist/app.js",
    "dev": "concurrently --kill-others \"tsc --watch\" \"npm run watchify\" \"npm run start\"",
    "watchify": "mkdir -p public/js && watchify dist/index.js -o public/js/bundle.js",
    "browserify": "mkdir -p public/js && browserify dist/index.js -o public/js/bundle.js",
    "postinstall": "./node_modules/.bin/tsc && npm run browserify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Menziess/Typescript-Reiskosten.git"
  },
  "author": "Stefan Schenk",
  "private": true,
  "dependencies": {
    "@types/vue": "^2.0.0",
    "tslint": "^5.8.0",
    "vue": "^2.5.13",
    "vuetify": "^0.17.6"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/express": "*",
    "@types/lodash": "*",
    "@types/mocha": "^2.2.47",
    "@types/node": "^8.5.7",
    "@types/supertest": "^2.0.4",
    "browserify": "^15.0.0",
    "chai": "^4.1.2",
    "concurrently": "^3.1.0",
    "express": "^4.0.35",
    "express-rewrite": "^0.0.4",
    "mocha": "^5.0.0",
    "supertest": "^3.0.0",
    "ts-node": "^4.1.0",
    "typescript": "^2.1.5",
    "typings": "^2.1.1",
    "watchify": "^3.10.0"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Menziess/Typescript-Reiskosten/issues"
  },
  "homepage": "https://github.com/Menziess/Typescript-Reiskosten#readme"
}
